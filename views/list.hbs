<div class="row mt-5">
    <div class="col-md-6 m-auto">
        <div class="card card-body">
            <ol class="list-group list-group-numbered">
                {{#each urlList}}
                <li class="list-group-item d-flex justify-content-between align-items-start">
                    <div class="ms-2 me-auto">
                        <div class="fw-bold">{{this.short}}</div>
                        <a href="{{this.origin}}">
                            {{this.origin}}
                        </a>
                    </div>
                </li>
                {{else}}
                <li class="l d-flex justify-content-between align-items-start">
                    <div class="ms-2 me-auto">
                        <div class="fw-bold">no Content</div>
                    </div>
                </li>
                {{/each}}
            </ol>
        </div>
    </div>
</div>